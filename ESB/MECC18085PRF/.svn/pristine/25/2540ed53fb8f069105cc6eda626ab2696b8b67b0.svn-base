  <xsd:complexType name="respuestaCeMotorEstatusLineaCaptura">
    <xsd:sequence>
      <xsd:element name="lc" type="xsd:string"/>
      <xsd:element name="importe" type="xsd:long"/>
      <xsd:element maxOccurs="unbounded" name="mensajes" type="tns:mensaje"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="mensaje">
    <xsd:sequence>
      <xsd:element name="clave" type="xsd:string"/>
      <xsd:element name="descripcion" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
