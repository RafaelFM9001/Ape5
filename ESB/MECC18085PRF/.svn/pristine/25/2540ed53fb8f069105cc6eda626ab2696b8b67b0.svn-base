  <xsd:complexType name="darEstatusLineaCapturaResponse">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RespuestaCeMotorLcEstatus" type="tns:respuestaCeMotorEstatusLineaCaptura"/>
    </xsd:sequence>
