    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="darEstatusLineaCaptura">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CeMotorLcEstatus" type="tns:ceMotorEstatusLineaCaptura"/>
