      <xsd:element name="tipoPago" type="xsd:int"/>
      <xsd:element name="identificador" type="xsd:string"/>
      <xsd:element name="referencia" type="xsd:string"/>
      <xsd:element name="vigenciaLC" type="xsd:dateTime"/>
      <xsd:element name="importe" type="xsd:long"/>
      <xsd:element name="tipoValidacion" type="xsd:int"/>
      <xsd:element name="digitosGlobales" type="xsd:int"/>
