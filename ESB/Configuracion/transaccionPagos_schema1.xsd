<?xml version='1.0' encoding='UTF-8'?>
<xs:schema version="1.0" targetNamespace="http://impl.webservice.util.web.wstran.ce.siat.sat.gob.mx/" xmlns:tns="http://impl.webservice.util.web.wstran.ce.siat.sat.gob.mx/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ConsultaTransaccion" nillable="true" type="xs:anyType"/>
  <xs:element name="WsTransException" type="tns:WsTransException"/>
  <xs:element name="consultaRespuestaTransaccion" type="tns:consultaRespuestaTransaccion"/>
  <xs:element name="consultaRespuestaTransaccionResponse" type="tns:consultaRespuestaTransaccionResponse"/>
  <xs:element name="generaTransaccion" type="tns:generaTransaccion"/>
  <xs:element name="generaTransaccionResponse" type="tns:generaTransaccionResponse"/>
  <xs:element name="guardaRespuestaTransaccion" type="tns:guardaRespuestaTransaccion"/>
  <xs:element name="guardaRespuestaTransaccionResponse" type="tns:guardaRespuestaTransaccionResponse"/>
  <xs:complexType name="generaTransaccion">
    <xs:sequence>
      <xs:element name="datosTransaccion" type="tns:datosTransaccion" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="datosTransaccion">
    <xs:sequence>
      <xs:element name="lc" type="xs:string" minOccurs="0"/>
      <xs:element name="cveFlujo" type="xs:int"/>
      <xs:element name="encriptacion" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="generaTransaccionResponse">
    <xs:sequence>
      <xs:element name="respuestaTransaccion" type="tns:respuestaTrans" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="respuestaTrans">
    <xs:sequence>
      <xs:element name="fecha" type="xs:string" minOccurs="0"/>
      <xs:element name="mensaje" type="xs:string" minOccurs="0"/>
      <xs:element name="numTransaccion" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="WsTransException">
    <xs:sequence>
      <xs:element name="error" type="xs:string" minOccurs="0"/>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
      <xs:element name="texto" type="xs:string" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="consultaRespuestaTransaccion">
    <xs:sequence>
      <xs:element name="consultaTransaccion" type="tns:consultaTransaccion" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="consultaTransaccion">
    <xs:sequence>
      <xs:element name="transaccion" type="xs:string"/>
      <xs:element name="cFlujo" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="consultaRespuestaTransaccionResponse">
    <xs:sequence>
      <xs:element name="respuestaTransaccion" type="tns:respuestaConsultaTransaccion" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="respuestaConsultaTransaccion">
    <xs:sequence>
      <xs:element name="transaccion" type="xs:string" minOccurs="0"/>
      <xs:element name="cFlujo" type="xs:int"/>
      <xs:element name="cveRespuesta" type="xs:string" minOccurs="0"/>
      <xs:element name="respuestaEncripcion" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="guardaRespuestaTransaccion">
    <xs:sequence>
      <xs:element name="respuestaTransaccion" type="tns:respTransaccionPagos" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="respTransaccionPagos">
    <xs:sequence>
      <xs:element name="numTransaccion" type="xs:string" minOccurs="0"/>
      <xs:element name="respuesta" type="xs:string" minOccurs="0"/>
      <xs:element name="respuestaEncriptacion" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="guardaRespuestaTransaccionResponse">
    <xs:sequence>
      <xs:element name="estatus" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>